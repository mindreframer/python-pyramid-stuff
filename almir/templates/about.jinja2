{% extends "templates/base.jinja2" %}

{% block container %}
<div class="span3">
 <div class="well">
  <ul class="nav nav-list">
    <li class="nav-header">Links</li>
    <li><a href="https://github.com/iElectric/almir/">Homepage</a></li>
    <li><a href="https://github.com/iElectric/almir/issues">Report a bug / Request a feature</a></li>
    <li><a href="http://readthedocs.org/docs/almir/">Documentation</a></li>
    <li><a href="https://groups.google.com/group/almir-bacula">Mailing list</a></li>
    <li><a href="https://groups.google.com/group/almir-announce">Announcements</a></li>
    <li><a href="irc://irc.freenode.net/almir">IRC</a></li>
  </ul>
 </div>
 <div class="well">
    <p>Donate to Almir! Contributions are applied as supplemental developer income.</p>
    <!-- paypal -->
    <form action="https://www.paypal.com/cgi-bin/webscr" method="post">
      <input type="hidden" name="cmd" value="_s-xclick">
      <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----
      ">
      <input type="image" src="https://www.paypalobjects.com/en_US/i/btn/btn_donateCC_LG.gif" border="0" name="submit" alt="PayPal - The safer, easier way to pay online!">
      <img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1">
    </form>
    <!-- end paypal -->

    <!-- flattr -->
    <script type="text/javascript">
    /* <![CDATA[ */
        (function() {
            var s = document.createElement('script'),
                t = document.getElementsByTagName('script')[0];
            s.type = 'text/javascript';
            s.async = true;
            s.src = 'http://api.flattr.com/js/0.6/load.js?mode=auto';
            t.parentNode.insertBefore(s, t);
        })();
    /* ]]> */</script>
    <a class="FlattrButton" style="display:none;" href="http://readthedocs.org/docs/almir/en/latest/"></a>
    <noscript><a href="http://flattr.com/thing/611637/Almir-bacula-web-interface" target="_blank">
    <img src="http://api.flattr.com/button/flattr-badge-large.png" alt="Flattr this" title="Flattr this" border="0" /></a></noscript>
    <!-- end flattr -->
 </div>
</div>

<div class="span9">
  <div class="hero-unit span10">
    {{ about|safe }}
  </div>

  <div class="hero-unit span10">
    {{ changelog|safe }}
  </div>
</div>
{% endblock %}
